# This file is a sample ndl script for a single operator (Plus) adding a constant to an input
# It is used by the Examples\Evaluation\CSEvalClient program
deviceId=0
run=AddOne
AddOne=[
          features = Input(1)
          v2 = Constant(1)
          ol = Plus(features, v2)

          FeatureNodes=(features)
          OutputNodes=(ol)
	]
