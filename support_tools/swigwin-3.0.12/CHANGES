	      %extend Foo {
	        int extension(int i, int j) { return i; }
		int extension() { return 1; }
	      }

	      %inline %{

	      class Foo {
	      public:
		int y;
		static const int z;
	      };
  
	      class Bar {
	        Foo *f;
	      public:
	        Bar(Foo *f) : f(f) { }
		Foo *operator->() {
		  return f;
		}
	      };

	      now you can 
	     
	      f = Foo()
	      f.y = 3
	      a = f.z
	      f->extension()
 
