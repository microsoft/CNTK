	{ $as_echo "$as_me:${as_lineno-$LINENO}: result: not found" >&5
$as_echo "not found" >&6; }
fi
else
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $TCLLIB" >&5
$as_echo "$TCLLIB" >&6; }
