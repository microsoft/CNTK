  withval=$with_jsv8lib; JSV8LIBDIR="$withval"
else
  JSV8LIB=
fi


  v8libdirs="$JSV8LIBDIR /usr/lib64/ /usr/local/lib64/ /usr/lib/ /usr/local/lib/"
