        { $as_echo "$as_me:${as_lineno-$LINENO}: result: not found" >&5
$as_echo "not found" >&6; }
      else
        { $as_echo "$as_me:${as_lineno-$LINENO}: result: $PIKEINCLUDE" >&5
$as_echo "$PIKEINCLUDE" >&6; }
      fi
fi
fi



