RootDir = .
ModelDir = "models"
command = "Simple_Test"

precision = "float"

modelPath = "$ModelDir$/ImageReaderGrayscale_Model.dnn"

# deviceId = -1 for CPU, >= 0 for GPU devices
deviceId = -1

outputNodeNames = "Dummy"
traceLevel = 1

Grayscale_Test = [
    # Parameter values for the reader
    reader = [
        # reader to use
        readerType = "ImageReader"
        file = "$RootDir$/ImageReaderGrayscale_map.txt"

        randomize = "auto"
        verbosity = 1
        
        grayscale = true
        
        numCPUThreads = 1
        features=[
            width=4
            height=4
            channels=1
            cropType=Center
            cropRatio=1.0
            jitterType=UniRatio
            interpolations=Linear
        ]
        labels=[
            labelDim=4
        ]
    ]
]
