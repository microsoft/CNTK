RootDir = .
ModelDir = "models"
command = "Regression_Test"

precision = double
labelType = regression

modelPath = "$ModelDir$/ImageReaderSimple_Model.dnn"

# deviceId = -1 for CPU, >= 0 for GPU devices
deviceId = -1

outputNodeNames = "Dummy"
traceLevel = 1

Regression_Test = [	
    # Parameter values for the reader
    reader = [
        # reader to use
        readerType = "ImageReader"
        file = "$RootDir$/ImageReaderSimpleRegression_map.txt"

        randomize = "auto"
        verbosity = 1

		numCPUThreads = 1
        features=[
            width=4
            height=4
            channels=1
        ]
        labels=[
            labelDim=2
        ]
    ]
]
