RootDir = .
ModelDir = "models"
command = "Simple_Test"

precision = "float"

modelPath = "$ModelDir$/ImageReaderSimple_Model.dnn"

# deviceId = -1 for CPU, >= 0 for GPU devices
deviceId = -1

outputNodeNames = "Dummy"
traceLevel = 1

Simple_Test = [
    # Parameter values for the reader
    reader = [
        # reader to use
        readerType = "ImageReader"
        file = "$RootDir$/ImageReaderBadMap_map.txt"

        randomize = "auto"
        verbosity = 1

        numCPUThreads = 1
        features=[
            width=4
            height=8
            channels=3
            cropType=Center
            sideRatio=1.0
            jitterType=UniRatio
            interpolations=linear
            #meanFile=$RootDir$/ImageReaderSimple_mean.xml
        ]
        labels=[
            labelDim=4
        ]
    ]
]
