#!/bin/bash

. $TEST_ROOT_DIR/run-test-common

OriginalTestDir=../../../../Examples/Speech/TIMIT/TrainMultiInput

# Note: can directly use run-timit-test-common since it lives at the same level
# in the directory hierarchy.
. $TEST_DIR/$OriginalTestDir/../run-timit-test-common

checkOriginalBaseline $TEST_DIR $OriginalTestDir

cntkrun TIMIT_TrainMultiInput_ndl_deprecated.cntk "TIMIT_TrainMultiInput=[reader=[readerType=HTKDeserializers]] $CntkArguments" || exit $?
